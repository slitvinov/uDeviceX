/* Compute angular momentum in a shear flow */

/*
w: 2 * 0.08;
D: [64, 32, 16 * (1 - w)];
[xh, yh, zh] :   D / 2;
[xl, yl, zl] : - D / 2; */

vx: gd*z;

ix(e):=integrate(e, x, xl, xh);
iy(e):=integrate(e, y, yl, yh);
iz(e):=integrate(e, z, zl, zh);
i3(e):= iz(iy(ix(e)));

V: i3(1);
I: i3(x^2 + z^2);
M: i3(vx * z);

factor(expand(M));
