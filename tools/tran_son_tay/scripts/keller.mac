a1: a;
a2: b;
a3: c;

r2: a2/a1;
r3: a3/a1;

al1: r2^(-1/3)*r3^(-1/3);
al2: r2^( 2/3)*r3^(-1/3);

D(s):= sqrt(    (al1^2 + s)*(al2^2+s)*(al3^2+s)   );

g3p: 'integrate( 1/(('al1^2+s)*('al2^2+s)*'D(s)),
  s, 0, +inf);

z1: 1/2*(r2^(-1) - r2);
z2: 'g0 * (al1^2 + al2^2);

f2p: 4*z1^2*(1-2/z2);
f3p: -4*z1/z2;
